{
  "Routes": [
    {
      //Donor.Api

      //DonorController
      "DownstreamPathTemplate": "/DonorApi/Donor/Login",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7232
        }
      ],
      "UpstreamPathTemplate": "/DonorApi/Donor/Login",
      "UpstreamHttpMethod": [ "Post" ]
    },
    {
      "DownstreamPathTemplate": "/DonorApi/Donor",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7232
        }
      ],
      "UpstreamPathTemplate": "/DonorApi/Donor",
      "UpstreamHttpMethod": [ "Get", "Post", "Put" ],
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "Bearer"
      }
    },
    {
      "DownstreamPathTemplate": "/DonorApi/Donor/{id}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7232
        }
      ],
      "UpstreamPathTemplate": "/DonorApi/Donor/{id}",
      "UpstreamHttpMethod": [ "Get", "Delete" ],
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "Bearer"
      }
    },
    {
      //DonorController
      "DownstreamPathTemplate": "/DonorApi/DonorRole",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7232
        }
      ],
      "UpstreamPathTemplate": "/DonorApi/DonorRole",
      "UpstreamHttpMethod": [ "Get", "Post", "Put"],
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "Bearer"
      }
    },
    {
      //DonorController
      "DownstreamPathTemplate": "/DonorApi/DonorRole/{id}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7232
        }
      ],
      "UpstreamPathTemplate": "/DonorApi/DonorRole/{id}",
      "UpstreamHttpMethod": [ "Get", "Delete" ],
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "Bearer"
      }
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "https://api.mybusiness.com"
  }
}